									>
								}
							}
							ELEMENT[at0008] occurrences matches {0..1} matches {    -- Beshaffenheit
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0009,    -- harter Stuhlgang
											at0010,    -- sehr fester Stuhlgang
											at0011,    -- fester bis weicher Stuhlgang
											at0012,    -- weicher Stuhlgang
											at0013,    -- weicher Stuhlgang
											at0014,    -- breiiger Stuhlgang
											at0015]    -- dünner Stuhlgang
										}
									}
								}
							}
						}
					}
					CLUSTER[at0016] occurrences matches {0..1} matches {    -- Miktion
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0017] occurrences matches {0..1} matches {    -- Häufigkeit
								value matches {
									C_DV_QUANTITY <
										
										list = <
											["1"] = <
												units = <"1/d">
												units_system = <"http://unitsofmeasure.org">
												units_display_name = <"1/d">
											>
										>
									>
								}
							}
							ELEMENT[at0018] occurrences matches {0..1} matches {    -- Beschwerden
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0019,    -- Brennen oder Schmerzen
											at0020,    -- Häufiger Harndrang
											at0021,    -- Starker, plötzlicher Harndrang
											at0022,    -- Schwacher Harnstrahl
											at0023,    -- Unterbrochener Harnstrahl
											at0024,    -- Gefühl der unvollständigen Entleerung
											at0025,    -- Nachträufeln
											at0026,    -- Harnverlust
											at0027]    -- Harnverhalt
										}
									}
								}
							}
							ELEMENT[at0028] occurrences matches {0..1} matches {    -- Kontinenz
								value matches {
									DV_BOOLEAN matches {*}
								}
							}
						}
					}
					CLUSTER[at0030] occurrences matches {0..1} matches {    -- Atmung
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0031] occurrences matches {0..1} matches {    -- Husten
								value matches {
									DV_BOOLEAN matches {*}
								}
							}
							ELEMENT[at0032] occurrences matches {0..1} matches {    -- Auswurf
								value matches {
									DV_BOOLEAN matches {*}
								}
							}
							ELEMENT[at0033] occurrences matches {0..1} matches {    -- DV_BOOLEAN
								value matches {
									DV_BOOLEAN matches {*}
								}
							}
						}
					}
					allow_archetype CLUSTER[at0041] occurrences matches {0..*} matches {    -- Körperliche Aktivität
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

